{
  "kind": "collectionType",
  "collectionName": "news_items",
  "info": {
    "singularName": "news-item",
    "pluralName": "news-items",
    "displayName": "news"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "Excerpt": {
      "type": "text"
    },
    "content": {
      "type": "blocks"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "pubichedAt": {
      "type": "date"
    },
    "Author": {
      "type": "string"
    },
    "Category": {
      "type": "string"
    }
  }
}
